
// the basic version, N philosophers

   
byte fork[15];








process phil_0 {
state think, one, eat, finish;
init think;
trans 
 think -> one {guard fork[0] == 0; effect fork[0] = 1;},
 one -> eat {guard fork[1] == 0; effect fork[1] = 1;},
 eat -> finish {effect fork[0] = 0; },
 finish -> think {effect fork[1] = 0; };
}

