process channel_ch_0 {
byte buf[12];
byte buf_act = 0;

state q;
init q;
trans
 q -> q { guard not(buf_act == 12); sync ch_0_in?buf[buf_act]; effect buf_act = buf_act+1; },
 q -> q { guard not(buf_act == 0); sync ch_0_out!buf[0]; effect buf[0] = buf[1], buf[1] = buf[2], buf[2] = buf[3], buf[3] = buf[4], buf[4] = buf[5], buf[5] = buf[6], buf[6] = buf[7], buf[7] = buf[8], buf[8] = buf[9], buf[9] = buf[10], buf[10] = buf[11],  buf[11] = 0, buf_act = buf_act -1;};
}

