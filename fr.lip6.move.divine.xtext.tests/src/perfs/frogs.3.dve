byte x= 0, y=15;



process Toad {
state q;
init q;
trans
 q -> q { guard x>0 && a[((y)*1+x-1)]==1; effect a[((y)*1+x)]=1, a[((y)*1+x-1)]=0, x=x-1;},
 q -> q { guard y>0 && a[((y-1)*1+x)]==1; effect a[((y)*1+x)]=1, a[((y-1)*1+x)]=0, y=y-1;},
 q -> q { guard x>1 && a[((y)*1+x-1)]==2 && a[((y)*1+x-2)]==1; effect a[((y)*1+x)]=1, a[((y)*1+x-2)]=0, x=x-2; },
 q -> q { guard y>1 && a[((y-1)*1+x)]==2 && a[((y-2)*1+x)]==1; effect a[((y)*1+x)]=1, a[((y-2)*1+x)]=0, y=y-2; };
}

process Frog {
state q;
init q;
trans
 q -> q { guard x<1-1 && a[((y)*1+x+1)]==2; effect a[((y)*1+x)]=2, a[((y)*1+x+1)]=0, x=x+1;},
 q -> q { guard y<31-1 && a[((y+1)*1+x)]==2; effect a[((y)*1+x)]=2, a[((y+1)*1+x)]=0, y=y+1;},
 q -> q { guard x<1-2 && a[((y)*1+x+1)]==1 && a[((y)*1+x+2)]==2; effect a[((y)*1+x)]=2, a[((y)*1+x+2)]=0, x=x+2; },
 q -> q { guard y<31-2 && a[((y+1)*1+x)]==1 && a[((y+2)*1+x)]==2; effect a[((y)*1+x)]=2, a[((y+2)*1+x)]=0, y=y+2; };
}

