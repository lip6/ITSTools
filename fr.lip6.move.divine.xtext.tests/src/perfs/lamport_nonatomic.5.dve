process NonatomicVar_0 {
byte x, v;
state q, r;
init q;
trans
 q -> q { sync read_0!x; },
 q -> r { sync write_0?v; },
 r -> q { sync done_0!; effect x = v; },
 r -> r { sync read_0!0; }, // anything can be read 
 r -> r { sync read_0!1; };
}
